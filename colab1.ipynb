# YouTube URL iÅŸleme ve ID Ã§Ä±karma fonksiyonu
def extract_youtube_id(url):
  """YouTube URL'sinden video ID'sini Ã§Ä±karÄ±r"""
  # & iÅŸareti varsa ilk kÄ±smÄ± al
  if '&' in url:
    url = url.split('&')[0]

  # Standart YouTube URL formatlarÄ±
  patterns = [
    r'(?:https?:\/\/)?(?:www\.)?youtube\.com\/watch\?v=([^\/\?\&]+)',  # youtube.com/watch?v=ID
    r'(?:https?:\/\/)?(?:www\.)?youtube\.com\/embed\/([^\/\?\&]+)',    # youtube.com/embed/ID
    r'(?:https?:\/\/)?(?:www\.)?youtube\.com\/v\/([^\/\?\&]+)',        # youtube.com/v/ID
    r'(?:https?:\/\/)?(?:www\.)?youtube\.com\/shorts\/([^\/\?\&]+)',   # youtube.com/shorts/ID
    r'(?:https?:\/\/)?(?:www\.)?youtu\.be\/([^\/\?\&]+)'               # youtu.be/ID
  ]

  # Her formatÄ± dene
  for pattern in patterns:
    match = re.search(pattern, url)
    if match:
      return match.group(1)

  # HiÃ§bir format eÅŸleÅŸmezse None dÃ¶ndÃ¼r
  return None

def download_youtube_video(youtube_url, output_path=None):
  """YouTube videosunu indirir (yt-dlp kullanarak) - Ses odaklÄ±, dÃ¼ÅŸÃ¼k gÃ¶rÃ¼ntÃ¼ kalitesi"""

  # Video ID'sini Ã§Ä±kar
  video_id = extract_youtube_id(youtube_url)
  if not video_id:
    raise ValueError(f"GeÃ§erli bir YouTube URL'si deÄŸil: {youtube_url}")

  print(f"YouTube video ID: {video_id}")
  print(f"Video indiriliyor (ses odaklÄ±, dÃ¼ÅŸÃ¼k gÃ¶rÃ¼ntÃ¼ kalitesi)...")

  # Ã‡Ä±ktÄ± dosya adÄ±nÄ± belirle
  if output_path is None:
    output_path = f"youtube_video_{video_id}.mp3"

  cmd = [
    'yt-dlp',
    '-f', 'bestaudio[ext=m4a]/bestaudio',  # Sadece en iyi ses kanalÄ±nÄ± indir
    '-o', output_path.replace('.mp4', '.m4a'),  # UzantÄ±yÄ± .m4a olarak deÄŸiÅŸtir
    youtube_url,
    '--no-warnings',
    '--quiet',
    '--extract-audio',  # Sadece ses kanalÄ±nÄ± Ã§Ä±kar
    '--audio-format', 'mp3',  # MP3 formatÄ±na dÃ¶nÃ¼ÅŸtÃ¼r
    '--audio-quality', '0',  # En yÃ¼ksek ses kalitesi
    '--force-ipv4',  # IPv4 kullan (bazen daha hÄ±zlÄ±)
    '--concurrent-fragments', '4'  # Paralel indirme parÃ§alarÄ±
  ]

  try:
    subprocess.run(cmd, check=True)

    # Video sÃ¼resini al
    duration_cmd = [
        'ffprobe',
        '-v', 'error',
        '-show_entries', 'format=duration',
        '-of', 'default=noprint_wrappers=1:nokey=1',
        output_path
    ]
    duration = float(subprocess.check_output(duration_cmd).decode('utf-8').strip())

    # Video bilgilerini gÃ¶ster
    print(f"Ä°Ã§erik baÅŸarÄ±yla indirildi: {output_path}")
    print(f"Ä°Ã§erik uzunluÄŸu: {duration:.1f} saniye ({format_time_duration(duration)})")

    return output_path
  except subprocess.CalledProcessError as e:
    raise Exception(f"Ä°ndirme hatasÄ±: {str(e)}")

def format_time(seconds):
  """Saniye cinsinden zamanÄ± SS:DD:SS formatÄ±na dÃ¶nÃ¼ÅŸtÃ¼rÃ¼r"""
  hours = int(seconds // 3600)
  minutes = int((seconds % 3600) // 60)
  seconds = int(seconds % 60)
  return f"{hours:02d}:{minutes:02d}:{seconds:02d}"

def format_time_duration(seconds):
  """Saniye cinsinden sÃ¼reyi okunabilir formata dÃ¶nÃ¼ÅŸtÃ¼rÃ¼r"""
  hours = int(seconds // 3600)
  minutes = int((seconds % 3600) // 60)
  secs = int(seconds % 60)

  if hours > 0:
    return f"{hours} saat {minutes} dakika {secs} saniye"
  elif minutes > 0:
    return f"{minutes} dakika {secs} saniye"
  else:
    return f"{secs} saniye"

