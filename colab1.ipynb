def get_video_duration(input_file):
  """Video veya ses dosyasÄ±nÄ±n sÃ¼resini saniye cinsinden dÃ¶ndÃ¼rÃ¼r"""
  cmd = [
      'ffprobe',
      '-v', 'error',
      '-show_entries', 'format=duration',
      '-of', 'default=noprint_wrappers=1:nokey=1',
      input_file
  ]
  output = subprocess.check_output(cmd).decode('utf-8').strip()
  return float(output)

def is_audio_file(file_path):
  """DosyanÄ±n ses dosyasÄ± olup olmadÄ±ÄŸÄ±nÄ± kontrol eder"""
  ext = os.path.splitext(file_path)[1].lower()
  return ext in AUDIO_FORMATS

def is_video_file(file_path):
  """DosyanÄ±n video dosyasÄ± olup olmadÄ±ÄŸÄ±nÄ± kontrol eder"""
  ext = os.path.splitext(file_path)[1].lower()
  return ext in VIDEO_FORMATS

def check_file_type(file_path):
  """Dosya tÃ¼rÃ¼nÃ¼ kontrol eder ve uygun deÄŸeri dÃ¶ndÃ¼rÃ¼r"""
  if is_audio_file(file_path):
    return "audio"
  elif is_video_file(file_path):
    return "video"
  else:
    return "unknown"

def optimize_for_whisper(input_path):
    """Video veya ses dosyasÄ±nÄ± Whisper iÃ§in optimize eder"""
    file_type = check_file_type(input_path)
    audio_path = f"{Path(input_path).stem}_audio.mp3"

    if file_type == "audio":
        # Ses dosyasÄ± - sadece optimize et
        cmd = [
            'ffmpeg',
            '-i', input_path,
            '-c:a', 'mp3',
            '-b:a', '192k',
            '-ar', '16000',
            '-ac', '1',
            '-af', 'highpass=f=200,lowpass=f=3000,volume=2',
            '-y',
            audio_path
        ]
    else:
        # Video dosyasÄ± - ses kanalÄ±nÄ± Ã§Ä±kar ve optimize et
        cmd = [
            'ffmpeg',
            '-i', input_path,
            '-vn',  # Video akÄ±ÅŸÄ±nÄ± devre dÄ±ÅŸÄ± bÄ±rak
            '-c:a', 'mp3',
            '-b:a', '192k',
            '-ar', '16000',
            '-ac', '1',
            '-af', 'highpass=f=200,lowpass=f=3000,volume=2',
            '-y',
            audio_path
        ]

    subprocess.run(cmd, stdout=subprocess.DEVNULL, stderr=subprocess.DEVNULL)
    return audio_path

def split_video_ffmpeg(input_file, segment_length=15, output_dir=None):
  """
  Video veya ses dosyasÄ±nÄ± FFmpeg kullanarak hÄ±zlÄ±ca belirtilen dakikalÄ±k segmentlere bÃ¶ler

  Args:
    input_file: Kaynak video veya ses dosyasÄ±
    segment_length: Her segmentin dakika cinsinden uzunluÄŸu
    output_dir: Ã‡Ä±ktÄ± dizini (None ise input_file'Ä±n bulunduÄŸu dizin kullanÄ±lÄ±r)

  Returns:
    OluÅŸturulan parÃ§a dosyalarÄ±nÄ±n listesi
  """
  if output_dir is None:
    output_dir = os.path.dirname(input_file)
    if output_dir == '':
      output_dir = '.'

  if not os.path.exists(output_dir):
    os.makedirs(output_dir)

  # Dosya sÃ¼resini al
  print(f"Dosya inceleniyor: {input_file}")
  total_duration = get_video_duration(input_file)

  # Segment sayÄ±sÄ±nÄ± hesapla
  segment_length_sec = segment_length * 60
  num_segments = int(total_duration / segment_length_sec) + (1 if total_duration % segment_length_sec > 0 else 0)

  print(f"Toplam sÃ¼re: {total_duration/60:.1f} dakika ({format_time_duration(total_duration)})")
  print(f"Dosya {num_segments} parÃ§aya bÃ¶lÃ¼necek ({segment_length} dakikalÄ±k dilimler)...")

  # Ã‡Ä±ktÄ± dosya listesi
  output_files = []

  # Dosya uzantÄ±sÄ±nÄ± al - aynÄ± uzantÄ±yÄ± Ã§Ä±ktÄ±lar iÃ§in kullan
  file_ext = os.path.splitext(input_file)[1]
  if not file_ext:
    file_ext = ".mp3"  # VarsayÄ±lan uzantÄ±

  # Her segment iÃ§in
  for i in range(num_segments):
    # Segment baÅŸlangÄ±Ã§ ve bitiÅŸ zamanÄ±
    start_time = i * segment_length_sec
    end_time = min((i + 1) * segment_length_sec, total_duration)

    # Dosya adÄ±
    base_name = os.path.splitext(os.path.basename(input_file))[0]
    output_file = os.path.join(output_dir, f"{base_name}_part{i+1:02d}{file_ext}")

    # FFmpeg ile segmenti kes
    print(f"BÃ¶lÃ¼m {i+1}/{num_segments} kesiliyor ({format_time(start_time)} - {format_time(end_time)})...")

    # FFmpeg komutu - stream kopyalama modu (-c copy) ile hÄ±zlÄ± kesme
    cmd = [
        'ffmpeg',
        '-i', input_file,
        '-ss', str(start_time),
        '-to', str(end_time),
        '-c', 'copy',  # Stream kopyalama - yeniden kodlama yok!
        '-y',  # Mevcut dosyalarÄ±n Ã¼zerine yaz
        output_file
    ]

    # Komutu Ã§alÄ±ÅŸtÄ±r
    subprocess.run(cmd, stdout=subprocess.DEVNULL, stderr=subprocess.DEVNULL)

    output_files.append(output_file)

  print(f"BÃ¶lme iÅŸlemi tamamlandÄ±. {len(output_files)} parÃ§a oluÅŸturuldu.")
  return output_files

def transcribe_segment(audio_path, model, language="tr", high_quality=True, timestamp_output=True):
  """Tek bir ses segmentinin transkriptini oluÅŸturur"""

  segment_start_time = time.time()
  print(f"Segment iÅŸleniyor: {os.path.basename(audio_path)}")

  try:
    # Transkript ayarlarÄ±
    options = {
      "language": language,
      "task": "transcribe",
      "verbose": False,
    }

    # YÃ¼ksek kalite iÃ§in ek parametreler
    if high_quality:
      options.update({
        "beam_size": 8,
        "best_of": 8,
        "temperature": [0.0, 0.2, 0.4, 0.6],
        "compression_ratio_threshold": 2.0,
        "condition_on_previous_text": True,
        "fp16": False  # YÃ¼ksek kalite iÃ§in FP32 kullan
      })

    # Bellek temizliÄŸi
    gc.collect()
    torch.cuda.empty_cache()

    # Transkript oluÅŸtur
    result = model.transcribe(audio_path, **options)

    # Transkript metnini al
    transcript_text = result["text"]

    # Ã‡Ä±ktÄ± dosya adÄ±nÄ± belirle
    output_path = f"{Path(audio_path).stem}_transkript.txt"

    # Transkripti dosyaya kaydet
    with open(output_path, "w", encoding="utf-8") as f:
      f.write(transcript_text)

      # EÄŸer segments bilgisi varsa ve zaman damgasÄ± istendiyse
      if timestamp_output and "segments" in result:
        f.write("\n\n## ZAMANLI TRANSKRÄ°PT ##\n\n")
        for segment in result["segments"]:
          segment_start = segment["start"]
          segment_end = segment["end"]
          text = segment["text"]
          timestamp = f"[{format_time(segment_start)} --> {format_time(segment_end)}]"
          f.write(f"{timestamp} {text}\n")

    # Ä°ÅŸlem sÃ¼resini hesapla
    segment_elapsed_time = time.time() - segment_start_time
    print(f"Segment tamamlandÄ±: {os.path.basename(output_path)}")
    print(f"Ä°ÅŸlem sÃ¼resi: {segment_elapsed_time:.2f} saniye ({format_time_duration(segment_elapsed_time)})")

    # Orijinal result'Ä± bellekten temizlemek iÃ§in sadece ihtiyacÄ±mÄ±z olan verileri saklayalÄ±m
    segments_copy = None
    if "segments" in result:
      # Segment verilerinin hafif bir kopyasÄ±nÄ± oluÅŸturalÄ±m
      segments_copy = []
      for segment in result["segments"]:
        segments_copy.append({
          "start": segment["start"],
          "end": segment["end"],
          "text": segment["text"]
        })

    # ArtÄ±k ihtiyacÄ±mÄ±z olmayan result verisini temizleyelim
    result = None

    return {
      "path": output_path,
      "text": transcript_text,
      "segments": segments_copy
    }
  except Exception as e:
    print(f"âŒ Segment iÅŸleme hatasÄ±: {str(e)}")
    print(f"HatalÄ± dosya: {audio_path}")
    import traceback
    traceback.print_exc()
    return {
      "path": f"{Path(audio_path).stem}_hata.txt",
      "text": f"Transkript oluÅŸturulamadÄ±: {str(e)}",
      "segments": None
    }

def merge_transcripts(transcript_results, input_file, timestamp_output=True):
  """TÃ¼m transkript sonuÃ§larÄ±nÄ± birleÅŸtirir ve bir dosyaya kaydeder"""

  # Ana video dosya adÄ±nÄ± al
  base_name = os.path.splitext(os.path.basename(input_file))[0]
  full_transcript_path = f"{base_name}_tam_transkript.txt"

  # TÃ¼m metinleri birleÅŸtir
  full_text = ""
  all_segments = []
  segment_time_offset = 0

  # Her segment iÃ§in
  for i, result in enumerate(transcript_results):
    # Tam metine bÃ¶lÃ¼m bilgisini ve transkripti ekle
    full_text += f"\n\n--- BÃ–LÃœM {i+1} ---\n\n"
    full_text += result["text"]

    # Zaman damgalÄ± segmentleri ayarla
    if timestamp_output and result["segments"] is not None:
      # Her segmentin zaman bilgisini orijinal videodaki konumuna gÃ¶re ayarla
      for segment in result["segments"]:
        # Yeni segment oluÅŸtur ve zaman bilgisini gÃ¼ncelle
        adjusted_segment = segment.copy()
        adjusted_segment["start"] += segment_time_offset
        adjusted_segment["end"] += segment_time_offset
        all_segments.append(adjusted_segment)

      # Bir sonraki bÃ¶lÃ¼m iÃ§in zaman farkÄ±nÄ± gÃ¼ncelle
      if result["segments"]:
        last_segment = result["segments"][-1]
        segment_time_offset += last_segment["end"]

  # Tam transkripti dosyaya kaydet
  with open(full_transcript_path, "w", encoding="utf-8") as f:
    # Ã–nce tam metni yaz
    f.write(full_text)

    # EÄŸer zaman damgalÄ± Ã§Ä±ktÄ± isteniyorsa ve segmentler varsa
    if timestamp_output and all_segments:
      # Zaman damgalÄ± transkripti ekle
      f.write("\n\n## TAM ZAMANLI TRANSKRÄ°PT ##\n\n")
      for segment in all_segments:
        start_time = segment["start"]
        end_time = segment["end"]
        text = segment["text"]
        timestamp = f"[{format_time(start_time)} --> {format_time(end_time)}]"
        f.write(f"{timestamp} {text}\n")

  print(f"Tam transkript oluÅŸturuldu: {full_transcript_path}")
  return full_transcript_path

# Ana iÅŸlev
def process_file(file_path, language="tr", model_size="large", high_quality=True,
                timestamp_output=True, segment_length_minutes=15, delete_segments_after=True):
  """
  Yerel veya indirilen dosyayÄ± iÅŸleyip transkript oluÅŸturur
  """
  try:
    total_start_time = time.time()

    # Dosya tÃ¼rÃ¼nÃ¼ kontrol et
    file_type = check_file_type(file_path)
    file_type_str = "Ses" if file_type == "audio" else "Video"

    # Optimize et (ses dosyasÄ±na dÃ¶nÃ¼ÅŸtÃ¼r)
    print(f"\n--- 1. {file_type_str.upper()} OPTÄ°MÄ°ZASYONU ---")
    audio_path = optimize_for_whisper(file_path)
    print(f"{file_type_str} dosyasÄ± optimize edildi: {audio_path}")

    # 2. Ses dosyasÄ±nÄ± parÃ§alara bÃ¶l
    print("\n--- 2. SES BÃ–LME (FFmpeg ile HÄ±zlÄ± Kesim) ---")
    audio_segments = split_video_ffmpeg(audio_path, segment_length=segment_length_minutes)

    # 3. GPU kontrolÃ¼
    device = "cuda" if torch.cuda.is_available() else "cpu"
    if device == "cuda":
      print(f"\nGPU kullanÄ±lÄ±yor: {torch.cuda.get_device_name(0)}")
    else:
      print("\nUYARI: GPU bulunamadÄ±! CPU kullanÄ±lacak (Ã§ok yavaÅŸ olabilir)")
      print("Large model iÃ§in GPU Ã¶nerilir. Colab > Ã‡alÄ±ÅŸma zamanÄ± > Ã‡alÄ±ÅŸma zamanÄ± tÃ¼rÃ¼nÃ¼ deÄŸiÅŸtir > GPU seÃ§eneÄŸini kullanÄ±n")

    # 4. Whisper modelini yÃ¼kle
